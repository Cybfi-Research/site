---
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
const { title } = Astro.props
---
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Cybfi Research</title>
  <meta name="viewport" content="width=device-width,initial-scale=1" />
  <meta name="description" content="We aim to improving children’s mental health through humane research and community outreach efforts that promote understanding." />
  <link rel="icon" type="image/png" sizes="48x48" href="icon-48x48.png">
  <link rel="apple-touch-icon" type="image/png" sizes="180x180" href="apple-touch-icon.png">
  <link rel="icon" type="image/ico" href="favicon.ico">
  <meta name="theme-color" content="#0080ff">
  <meta property="og:title" content="Cybfi Research" />
  <meta property="og:description" content="We aim to improving children’s mental health through humane research and community outreach efforts that promote understanding." />
  <meta name="twitter:site" content="https://cybfi.org" />
  <meta name="twitter:title" content="Cybfi Research" />
  <meta name="twitter:description" content="We aim to improving children’s mental health through humane research and community outreach efforts that promote understanding." />
</head>

<body class="mx-10">
  <Header />
  <slot/>
  <Footer />
</body>
</html>